{
    "name": "corpus/loggers",
    "description": "Helpful PSR-3 Utility Loggers",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "psr-3",
        "logger",
        "log",
        "logging"
    ],
    "require": {
        "psr/log": "^1 || ^2 || ^3",
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "donatj/drop": "^1.1",
        "phpunit/phpunit": "^9.6",
        "donatj/mddoc": "^0.10.1",
        "squizlabs/php_codesniffer": "^3.13",
        "corpus/coding-standard": "^0.8.0",
        "friendsofphp/php-cs-fixer": "^3.65",
        "rawr/phpunit-data-provider": "^3.3",
        "corpus/recursive-require": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Corpus\\Loggers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corpus\\Loggers\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Jesse Donat",
            "email": "donatj@gmail.com"
        }
    ],
    "provide": {
        "psr/log-implementation": "1.0|2.0|3.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
